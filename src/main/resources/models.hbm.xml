<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="trainSimulator.models">
    <class name="EventLog">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="type"/>
        <property name="timestamp"/>
        <property name="stationName"/>
        <property name="comment"/>
    </class>
    <class name="GeneratorParameter">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="parameterName"/>
        <property name="parameterValue"/>
    </class>
    <class name="Passenger">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="tickets"/>
        <property name="trainID"/>
    </class>
    <class name="Route">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="stationsOnRoute"/>
        <property name="trainsOnRoute"/>
        <property name="isAvailable"/>
    </class>
    <class name="Station">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="trainsOnStation"/>
        <property name="route"/>
        <property name="timetableForStation"/>
    </class>
    <class name="Ticket">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="passenger"/>
        <property name="price"/>
    </class>
    <class name="TimetableEntity">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="getTrain"/>
        <property name="station"/>
        <property name="arrivalTime"/>
        <property name="departureTime"/>
    </class>
    <class name="Train">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="routeId"/>
        <property name="timetable"/>
        <property name="passengers"/>
        <property name="currentStation"/>
    </class>
</hibernate-mapping>